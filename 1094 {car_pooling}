#include <stdbool.h>

bool carPooling(int** trips, int tripsSize, int* tripsColSize, int capacity) {
    int diff[1001] = {0};

    for (int i = 0; i < tripsSize; i++) {
        int num = trips[i][0];
        int from = trips[i][1];
        int to = trips[i][2];

        diff[from] += num;
        diff[to]   -= num;
    }

    int curr = 0;
    for (int i = 0; i <= 1000; i++) {
        curr += diff[i];
        if (curr > capacity) return false;
    }
    return true;
}
